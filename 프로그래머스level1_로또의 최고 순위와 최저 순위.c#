using System;

    public class Solution
    {
        public int[] solution(int[] lottos, int[] win_nums)
        {
            int[] answer = new int[2];
            Array.Sort(lottos);
            Array.Sort(win_nums);
            int cnt_max = 0;
            int cnt_min = 0;

            for(int i = 0; i < 6; i++)
            {
                for(int j = 0; j < 6;j++)
                {
                    if (lottos[j] == win_nums[i])
                    {
                        cnt_max++;
                        cnt_min++;
                    }
                    
                }
                if (lottos[i] == 0)
                    cnt_max++;
            }
            cnt_max = cnt_max;
            answer[0] = 7 - cnt_max;
            answer[1] = 7 - cnt_min;
            if (answer[0] >= 6)
            {
                answer[0] = 6;
            }
            if (answer[1] >= 6)
            {
                answer[1] = 6;
            }
            return answer;
        }
    }
